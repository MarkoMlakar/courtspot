{
  "name": "api",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "ts-node -r dotenv/config node_modules/knex/bin/cli.js migrate:latest --knexfile src/database/knexfile.ts",
    "migrate:rollback": "ts-node -r dotenv/config node_modules/knex/bin/cli.js migrate:rollback --knexfile src/database/knexfile.ts",
    "migrate:make": "ts-node -r dotenv/config node_modules/knex/bin/cli.js migrate:make --knexfile src/database/knexfile.ts",
    "seed": "ts-node -r dotenv/config node_modules/knex/bin/cli.js seed:run --knexfile src/database/knexfile.ts",
    "seed:make": "ts-node -r dotenv/config node_modules/knex/bin/cli.js seed:make --knexfile src/database/knexfile.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "mysql2": "^3.14.1",
    "objection": "^3.1.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.1",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
